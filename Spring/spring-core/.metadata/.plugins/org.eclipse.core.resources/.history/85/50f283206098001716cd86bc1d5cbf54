package org.koushik.practice.spring;

public class Triangle {

	private String type;
	
	
	public String getType() {
		return type;
	}


	public void setType(String type) {
		this.type = type;
	}

	
	//Through setter injection 
	
	public void draw(){
		System.out.println(getType() +" Triangle drawn");
	}
}
