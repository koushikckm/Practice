@Path("/")
public class CommentsController {

	@GET
	public String testSubResource(){
		return "new sub resource";
	}
	
	@GET
	@Path("/{commentId}")
	public String testSubResource1(@PathParam("commentId") long commentId, @PathParam("messageId") long messageId ){
		return "Another sub resource";
	}
}
